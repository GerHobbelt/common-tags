{
  "name": "common-tags",
  "description": "a few common utility template tags for ES2015",
  "version": "0.0.3",
  "keywords": [
    "es2015",
    "es6",
    "babel",
    "template",
    "tag",
    "tagged",
    "literal",
    "string",
    "strings",
    "multi",
    "single",
    "one",
    "line",
    "multiline",
    "singleline",
    "oneline",
    "es6-tag",
    "es2015-tag",
    "strip",
    "indent",
    "indents",
    "normalize",
    "array"
  ],
  "scripts": {
    "lint": "snazzy",
    "build": "babel src -d lib",
    "release": "npm publish",
    "prerelease": "mv lib src && babel src -d lib --stage 0",
    "postrelease": "rm -rf lib && mv src lib",
    "test": "npm run lint && nyc ava",
    "coverage": "nyc report --reporter=lcov",
    "coveralls": "coveralls < coverage/lcov.info",
    "precoveralls": "npm run coverage"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "test",
      "readme.md"
    ]
  },
  "ava": {
    "verbose": true,
    "babel": "inherit",
    "require": [
      "babel-register"
    ],
    "files": [
      "src/**/*.test.js"
    ]
  },
  "main": "lib",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/declandewet/common-tags"
  },
  "bugs": {
    "url": "http://github.com/declandewet/common-tags/issues",
    "email": "declandewet@me.com"
  },
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.8.0",
    "nyc": "^6.4.4",
    "snazzy": "^4.0.0",
    "when": "^3.7.7"
  }
}
